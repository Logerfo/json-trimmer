{
    "name": "json-trimmer",
    "displayName": "JSON Trailing Commas Trimmer",
    "description": "Trims trailing commas in json files.",
    "version": "0.0.3",
    "publisher": "logerfo",
    "engines": {
        "vscode": "^1.28.0"
    },
    "bugs": {
        "url": "https://github.com/Logerfo/json-trimmer/issues",
        "email": "bruno@logerfo.tk"
    },
    "author": {
        "email": "bruno@logerfo.tk",
        "name": "Bruno Logerfo",
        "url": "https://github.com/Logerfo"
    },
    "license": "Unlicense",
    "homepage": "https://github.com/Logerfo/json-trimmer/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Logerfo/json-trimmer.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:json",
        "onLanguage:jsonc"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "json-trimmer.trim",
                "title": "Trim Trailing Commas"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "webpack --mode none",
        "watch": "webpack --mode none --watch",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/mocha": "5.2.6",
        "@types/node": "11.13.9",
        "ts-loader": "6.0.0",
        "tslint": "5.16.0",
        "typescript": "3.4.5",
        "vscode": "1.1.34",
        "webpack": "4.31.0",
        "webpack-cli": "3.3.2"
    }
}
